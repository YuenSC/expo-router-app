{
  "cli": {
    "version": ">= 9.1.0",
    "appVersionSource": "local"
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "dev",
        "EXPO_PUBLIC_API_URL": "https://group-expense-628faa35624f.herokuapp.com/"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "preview",
        "EXPO_PUBLIC_API_URL": "https://group-expense-628faa35624f.herokuapp.com/"
      },
      "ios": {
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "prod",
        "EXPO_PUBLIC_API_URL": "https://group-expense-628faa35624f.herokuapp.com/"
      },
      "ios": {
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
